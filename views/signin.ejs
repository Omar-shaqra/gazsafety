/*start main rules*/
* {
	padding: 0;
	margin: 0;
	font-family: "Roboto", sans-serif;
}
.container{
	width:70%;
	margin:auto;
}
/*end main rules*/

/*start header*/
.header .navbar{
	background-color:#252F31;
	color:#fff;
	overflow:hidden;
	text-transform:uppercase;
}
.header .navbar .container{width:90%;}
.header .navbar .container a {
	text-decoration: none;
	color: #FFF;
}
.header .navbar img {
	width:70px;
	height:70px;
