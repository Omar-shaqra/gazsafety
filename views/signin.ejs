</head>
<body>
  <!--Start Header-->
  <div class="header">
      <div class="navbar">
          <div class="container">
              <a href="/">
                  <img src="../images/Logo.png" />
                  <h2>Gas<span>Pro</span></h2>
              </a>
              <ul>
                  <li><a href="/">Home</a></li>
                  <li><a href="../About Gaspro/about.html">About gaspro</a></li>
                  <li><a href="../schedule service/service.html">schedule service</a></li>
                  <li><a href="../Our community/our.html">our community</a></li>
              </ul>
          </div>
      </div>
  </div>
  <!--End Header-->

  <!-- Start sign in body -->

  <div class="scontainer">
      <div class="box">
        	<h1 style="color:red "><%=err%></h1>
      <h1>Sign In</h1>
      <form  method="post">


      <div class="row">
          <input type="text" name="name" placeholder="Username" required>
      </div>
      <div class="row">
          <input type="password" name="pass" placeholder="Password" required>
      </div>
      <div class="row">
        <a id="login" href="#"><input type="submit" name="" value="Log In"></a>
      </div>

        </form>
      </div>
  </div>

  <!-- End sign in body -->
</body>
