/*start main rules*/
* {
	padding: 0;
	margin: 0;
	font-family: "Roboto", sans-serif;
}
.container{
	width:70%;
	margin:auto;
}
/*end main rules*/

/*start header*/
.header .navbar{
	background-color:#252F31;
	color:#fff;
	overflow:hidden;
	text-transform:uppercase;
}
.header .navbar .container{width:90%;}
.header .navbar .container a {
	text-decoration: none;
	color: #FFF;
}
.header .navbar img {
	width:70px;
	height:70px;
	float: left;
	padding-bottom:5px;

}
.header .navbar h2 {
	float: left;
	padding-top: 10px;
	margin-top:15px;
}
.header .navbar h2 span {color: #06a5b8;}
.header .navbar ul {
	list-style: none;
	padding-left: 0;
	padding-top: 10px;
	overflow: hidden;
	float: right;
}
.header .navbar ul li{
	float:left;
	padding:10px;
	margin-top:15px;
}
.header .navbar ul li a{
	text-decoration:none;
	color:#FFF;
}
.header .navbar ul li a:hover {color: #06a5b8;}

/*end header*/

/*Start sign in*/

@import url('https://fonts.googleapis.com/css2?family=Quicksand:wght@500&display=swap');
*{
    padding: z0;
    margin: 0;
    font-family: 'Quicksand', sans-serif;
   }
body{
    background: #fff;
}
.scontainer{
    position: relative;
    width: 100%;   min-height: 100vh;
    background: url(https://mk0geketeyorgkvhgr38.kinstacdn.com/wp-content/uploads/2015/12/city-road-street-buildings-1024x673.jpg);
    /* background: url('../../images/slider.png'); */
    /* background-size: 100% 100%; */
    background-size: cover;
}
.scontainer::before{
    content: '';
    position: absolute;
    top: 0;  left: 0;
    background: #262626;
    width: 100%;  height: 100%;
    mix-blend-mode: hard-light;
}
.scontainer .box{
    position: absolute;
    top: 50%;  left: 50%;
    transform: translate(-50%,-50%);
    width: 50vw;  min-height: 10vh;
    background: transparent;  color: #fff;
    padding: 50px 20px;
    box-sizing: border-box;
    transition: .9s;
}
/* .scontainer .box:hover{
    background: rgba(255,255,255,.2);
} */
.scontainer .box h1{
    font-size: 40px;
    text-align: center;
    text-transform: uppercase;
}
.scontainer .box .row{
    width: 100%;
    margin: 25px 0;
    text-align: center;
}
.scontainer .box .row input{
    border: none;   outline: none;
    width: 100%;
    padding: 13px 25px;
    border-radius: 30px;
    box-sizing: border-box;
    background: rgba(255,255,255,.4);   color: #fff;
    font-size: 18px;
}
::placeholder{
    color: #bbb;
/*     text-transform: uppercase; */
}
.scontainer .box .row a{
    display: inline-block;
    padding: 10px 0;
    color: #fff;
    text-align: center;
    text-decoration: none;  text-transform: uppercase;
    font-size: 18px;
}
.scontainer .box .row a#login{
    background: #00bcd4;
    padding: 15px 0;
    border-radius: 50px;
    width: 100%;
}
/* .scontainer .box .row a#forgetpass{
	color: #d9534f;
} */
.scontainer .box .row a#forgetpass:hover{
	color: #d9534f;
	text-decoration: underline;
}
/*End sign in*/
